<div class="left-sidenav">
    <!-- LOGO -->
    <div class="brand">
        <a asp-controller="home" asp-action="index" class="logo">
            <span>
                <img src="~/images/logo.png" alt="logo-large" class="logo-lg logo-light">
                <img src="~/images/uwingoLogo.jpg" alt="logo-large" class="logo-lg logo-dark" style="width:100px; height:auto;">
            </span>
        </a>
    </div>
    <!--end logo-->
    <div class="menu-content h-100" data-simplebar>
        <ul class="metismenu left-sidenav-menu">
            <li class="menu-label mt-0">@Localizer["Main"]</li>
            <li>
                <a href="javascript: void(0);">
                    <i data-feather="home" class="align-self-center menu-icon"></i>
                    <span>@Localizer["Dashboard"]</span>
                    <span class="menu-arrow"><i class="mdi mdi-chevron-right"></i></span>
                </a>
                <ul class="nav-second-level" aria-expanded="false">
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="home" asp-action="index">
                            <i class="ti-control-record"></i>@Localizer["HomePage"]
                        </a>
                    </li>
                </ul>
            </li>

            <li>
                <a href="javascript: void(0);">
                    <i data-feather="file-plus" class="align-self-center menu-icon"></i>
                    <span>@Localizer["Pages"]</span>
                    <span class="menu-arrow"><i class="mdi mdi-chevron-right"></i></span>
                </a>
                <ul class="nav-second-level" aria-expanded="false">
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="brand">
                            <i class="ti-control-record"></i>@Localizer["Brands"]
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="model">
                            <i class="ti-control-record"></i>@Localizer["Models"]
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="measurementUnit">
                            <i class="ti-control-record"></i>@Localizer["MeasurementUnits"]
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="supplier">
                            <i class="ti-control-record"></i>@Localizer["Suppliers"]
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="consumable">
                            <i class="ti-control-record"></i>@Localizer["Consumables"]
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="product">
                            <i class="ti-control-record"></i>@Localizer["Products"]
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="productType">
                            <i class="ti-control-record"></i>@Localizer["ProductTypes"]
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="warehouse">
                            <i class="ti-control-record"></i>@Localizer["Warehouses"]
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="inventory">
                            <i class="ti-control-record"></i>@Localizer["Inventories"]
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="index" asp-controller="stockchange">
                            <i class="ti-control-record"></i>@Localizer["StockChanges"]
                        </a>
                    </li>
                </ul>
            </li>
        </ul>

        <div class="update-msg text-center">
            <a href="javascript: void(0);" class="float-end close-btn text-muted" data-dismiss="update-msg" aria-label="Close" aria-hidden="true">
                <i class="mdi mdi-close"></i>
            </a>
            <h5 class="mt-3">@Localizer["UwingoAcademy"]</h5>
            <p class="mb-3">@Localizer["TatvanMunicipalityInventoryManagement"]</p>
          
        </div>
    </div>
</div>